<div class="row">
    <div class="col-md-8 col-md-offset-2">
        <h1>User Profile</h1>
        <br>

        {{# each orders }}
            <div class="panel panel-default">

                <div class="panel-body">
                    
                    <hr>
                    <ul class="list-group">
                        {{# each this.items }}
                            <li class="list-group-item">
                                <span class="badge badge-pill badge-dark">{{ this.price }}</span> <strong>{{ this.item.title }}</strong> <span class="badge badge-pill badge-success pull-right"> {{this.qty }}</span>
                            </li>
                        {{/each}}
                    </ul>
                </div>

                <div class="panel-footer">
                    <br>
                    <h4><strong> Total: Rs.{{ this.cart.totalPrice }}/-</strong></h4>
                </div>

            </div>
        {{/each}}
    </div>
</div>